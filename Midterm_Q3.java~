import java.util.*;
public class Midterm_Q3 {
  
  
  public static void main(String[] args) { 
    String s;
    
    Scanner scan=new Scanner(System.in);
    System.out.println("Enter your letter");
    s=scan.nextLine();
    
    char ch=s.charAt(0);
    
    if(ch=='A' || ch=='B' || ch=='C')
      System.out.println("The digit 2 corresponds to the letter "+s+" on the telephone");
    if(ch=='D' || ch=='E' || ch=='F')
      System.out.println("The digit 3 corresponds to the letter "+s+" on the telephone");
    if(ch=='G' || ch=='H' || ch=='I')
      System.out.println("The digit 4 corresponds to the letter "+s+" on the telephone");
    if(ch=='J' || ch=='K' || ch=='L')
      System.out.println("The digit 5 corresponds to the letter "+s+" on the telephone");
    if(ch=='M' || ch=='N' || ch=='O')
      System.out.println("The digit 6 corresponds to the letter "+s+" on the telephone");
    if(ch=='P' || ch=='R' || ch=='S')
      System.out.println("The digit 7 corresponds to the letter "+s+" on the telephone");
    if(ch=='T' || ch=='U' || ch=='V')
      System.out.println("The digit 8 corresponds to the letter "+s+" on the telephone");
    if(ch=='W' || ch=='X' || ch=='Y')
      System.out.println("The digit 9 corresponds to the letter "+s+" on the telephone");
    else if(ch=='Q' || ch=='Z')
      System.out.println("These digit are not there in telephone");
    
    
  }
  
